{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": { "distDir": ".svelte-kit/vercel" }
		},
		{
			"src": "server/api/index.py",
			"use": "@vercel/python"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "server/api/index.py"
		},
		{
			"src": "/(.*)",
			"dest": "/"
		}
	]
}
